{
  "name": "@vsh/sakura",
  "version": "0.1.0",
  "exports": {
    ".": "./package/mod.ts",
    "./error": "./package/error.ts",
    "./res": "./package/res.ts",
    "./router": "./package/router.ts",
    "./server": "./package/server.ts"
  },
  "publish": {
    "exclude": ["./tests", ".rest"]
